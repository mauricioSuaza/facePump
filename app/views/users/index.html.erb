<ul>

  <% @users.each do |user| %>
  <% if user!= current_user %>

    <li>
      <% if user.avatar.url.present? %>
      <%= image_tag(user.avatar.url, height: '64', width: '64') %>
      <%end%>
    </li>

    <li> <a href="<%=user_path(user)%>"><%= user.name %></a></li>

  <%end%>


  <% end %>
</ul>
